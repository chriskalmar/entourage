{
  "name": "entourage-cli",
  "version": "0.3.0",
  "description": "Client for entourage",
  "main": "lib/",
  "bin": {
    "entourage-cli": "./bin/entourage-cli.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "coverage": "NODE_ENV=test jest --runInBand --coverage",
    "coverage-ci": "npm run coverage && cat ./coverage/lcov.info | codecov",
    "build": "mkdir -p lib; npm run test && NODE_ENV=build babel src -d lib --verbose",
    "build-watch": "mkdir -p lib; NODE_ENV=build babel src -w -d lib --verbose",
    "dev": "babel-node src/",
    "start": "NODE_ENV=production node src/index.js",
    "release": "np --no-publish",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "bin"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "test",
    "tests",
    "automation",
    "integration",
    "ci"
  ],
  "author": "Chris Kalmar <christian.kalmar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/runtime": "7.9.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.5.1",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "husky": "4.2.3",
    "jest": "25.5.4",
    "lerna": "3.20.2",
    "lint-staged": "10.0.8",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "prettier-eslint": "9.0.1",
    "rimraf": "3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "axios": "^0.19.0",
    "graphql-mqtt-subscriptions": "^1.2.0",
    "lodash": "^4.17.15",
    "mqtt": "^3.0.0",
    "yargs": "^15.0.2"
  }
}
