{
  "name": "entourage-cli",
  "version": "0.1.0",
  "description": "Client for entourage",
  "main": "lib/",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "coverage": "NODE_ENV=test jest --runInBand --coverage",
    "coverage-ci": "npm run coverage && cat ./coverage/lcov.info | codecov",
    "build": "mkdir -p lib; npm run test && NODE_ENV=build babel src -d lib --verbose",
    "build-watch": "mkdir -p lib; NODE_ENV=build babel src -w -d lib --verbose",
    "dev": "babel-node src/",
    "start": "NODE_ENV=production node src/index.js",
    "release": "np --no-publish",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": "lint-staged",
  "keywords": [
    "test",
    "tests",
    "automation",
    "integration",
    "ci"
  ],
  "author": "Chris Kalmar <christian.kalmar@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.7.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "codecov": "^3.6.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "yargs": "^15.0.2"
  }
}
